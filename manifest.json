 {
    "name": "Harambe's Toolkit",
    "version": "1.0",
    "description": "Quickly share current tab to ARCHIVE.IS, ARCHIVE.TODAY, ARCHIVE.ORG, DISSENTER, or TWITTER with the lightweight Harambe's Toolkit",
    "permissions": ["storage", "activeTab", "declarativeContent", "bookmarks"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "32": "harambe-32.png",
        "128": "harambe-128.png"
    }
    },
    "options_page": "options.html",
    "manifest_version": 2
  }
